# Gemini AI API Key (Server-side only)
GEMINI_API_KEY=your_api_key_here

# Default languages
NEXT_PUBLIC_DEFAULT_SOURCE_LANG=en-US
NEXT_PUBLIC_DEFAULT_TARGET_LANG=es-ES

# Security Settings
NODE_ENV=production
NEXT_PUBLIC_API_URL=https://your-production-url.com

# HIPAA Compliance Settings
ENCRYPTION_KEY=your_32_byte_encryption_key
AUDIT_LOG_RETENTION_DAYS=7
MAX_SESSION_DURATION_MINUTES=15
SECURE_AUDIT_LOG_PATH=/secure/audit/logs
HIPAA_COMPLIANCE_MODE=strict

# Logging Service (Choose one: cloudwatch, azure-monitor, stackdriver)
LOG_SERVICE=cloudwatch
LOG_SERVICE_API_KEY=your_log_service_api_key 